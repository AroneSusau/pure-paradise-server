{
  "name": "pure-paradise-v2",
  "version": "0.0.1",
  "description": "A text based RPG video game, originally created for a university assignment in Java, ported to Javascript / TypeScript",
  "main": "src/public/index.ts",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm run build && node build/private/index.js",
    "clean": "rm -rf ./build",
    "build": "npm run clean && tsc && cd src && rsync -R `find . \\( -name '*.css' -or -name '*.html' -or -name '*.ico' \\)` ../build/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AroneSusau/pure-paradise-v2.git"
  },
  "keywords": [
    "RPG",
    "game",
    "assignment",
    "learning"
  ],
  "author": "Arone Tie Susau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AroneSusau/pure-paradise-v2/issues"
  },
  "homepage": "https://github.com/AroneSusau/pure-paradise-v2#readme",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.1",
    "@types/socket.io": "^2.1.4",
    "express": "^4.17.1",
    "socket.io": "^2.3.0"
  }
}
